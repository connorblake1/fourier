class twoDim {
  int[] x;
  int[] y;
  int in;
  twoDim(int[] x1, int[] y1, int i) {
    x = x1;
    y = y1;
    in = i;}
  int[] getXA() {
    return x;}
  int[] getYA() {
    return y;}
  void setXA(int[] x1) {
    x = x1;}
  void setYA(int[] y1) {
    y = y1;}
  int getIn() {
    return in;}
  void setIn(int i) {
    in = i;}}
